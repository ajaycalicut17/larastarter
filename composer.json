{
    "name": "ajaycalicut17/meadow",
    "description": "Meadow is a laravel package used for instant admin panel.",
    "type": "library",
    "keywords": [
        "ajaycalicut17",
        "laravel",
        "meadow"
    ],
    "homepage": "https://github.com/ajaycalicut17/meadow",
    "license": "MIT",
    "authors": [
        {
            "name": "ajaycalicut17",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/fortify": "^1.10",
        "livewire/livewire": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Ajaycalicut17\\Meadow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ajaycalicut17\\Meadow\\MeadowServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}